<?php

/**
 * @file for installing the module requirements
 */

use Drupal\webform\Entity\Webform;

/**
 * Method to delete webform inbuilt contact form if it exists.
 */
function ah_webform_install() {
  $inbuilt_contact_webform = Webform::load('contact');
  if ($inbuilt_contact_webform) {
    $inbuilt_contact_webform->delete();
  }
}
